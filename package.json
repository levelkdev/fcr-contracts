{
  "name": "sol-tcr",
  "version": "0.0.1",
  "description": "A generic, string-keyed TCR in Solidity",
  "scripts": {
    "install": "truffle install",
    "compile": "truffle compile",
    "solc-compile": "node scripts/solcCompile.js",
    "coverage": "solidity-coverage",
    "deploy-local": "truffle migrate --reset",
    "deploy-rinkeby": "truffle migrate --reset --network rinkeby",
    "deploy-mainnet": "truffle migrate --network mainnet",
    "flatten": "node scripts/flattenContracts.js",
    "test:truffle": "run-with-testrpc -l 20000000 'truffle test --network unit_testing'",
    "test": "truffle test --network=unit_testing",
    "integration-test": "truffle exec ./test/helpers/execIntegrationTest.js",
    "integration-tests": "scripts/integrationTests.sh",
    "migrate": "truffle migrate",
    "migrate-reset": "npm run clean-truffle-config && truffle migrate --reset",
    "fix": "eslint --fix",
    "lint": "eslint",
    "ganache": "scripts/runGanache.sh",
    "truffle-version": "truffle version",
    "clean-truffle-config": "node scripts/cleanTruffleConfig.js"
  },
  "author": "Irene Lin <irene.lin@consensys.net>, Mira Zeitlin <miranda.zeitlin@consensys.net>, Yorke Rhodes <yorke.rhodesiv@consensys.net>",
  "license": "ISC",
