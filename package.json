    "@gnosis.pm/util-contracts": "git://git@github.com:levelkdev/util-contracts#c8c4c50a8474fd99b9b576bcc0bf42a0505f3752",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
